<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- application name and title of error dialogs-->
    <string name="app_name">"Záznam zvuku"</string>
    <string name="folder_name">"Zaznamenávanie"</string>
    <!-- Screen title before and during recording -->
    <string name="record_your_message">"Zaznamenajte svoju správu"</string>
    <!-- Screen title after recording -->
    <string name="message_recorded">"Správa bola zaznamenaná"</string>
    <!-- Screen title while playing back a recording -->
    <string name="review_message">"Skontrolovať správu"</string>
    <!-- Label shown during recording -->
    <string name="recording">"Prebieha zaznamenávanie"</string>
    <!-- Label shown when recording is paused -->
    <string name="recording_paused">"pozastavené"</string>
    <!-- Label shown when the recording is stopped for a reason other than the user stopping it (e.g. the sd card was removed) -->
    <string name="recording_stopped">"Záznam bol zastavený"</string>
    <!-- label shown when there is not enough space to record something -->
    <string name="storage_is_full">"Ukladací priestor je plný"</string>
    <!-- label shown when the recording has reached maximum allowed file size -->
    <string name="max_length_reached">"Bola dosiahnutá maximálna povolená dĺžka"</string>
    <!-- label shown when there is no sd card available to record to [CHAR LIMIT=30] -->
    <string name="insert_sd_card" product="nosdcard">"Pripojiť úložisko USB"</string>
    <!-- label shown when there is no sd card available to record to -->
    <string name="insert_sd_card" product="default">"Vložte kartu SD"</string>
    <!-- label shown when there is more than 1 minute but less than 9 minutes of space left to record -->
    <string name="min_available">"K dispozícii <xliff:g id="MINUTES">%d</xliff:g> min"</string>
    <!-- label shown when there is less than 1 minute of space left to record -->
    <string name="sec_available">"K dispozícii <xliff:g id="SECONDS">%d</xliff:g> s"</string>
    <!-- button to accept the current recording and return it to the caller -->
    <string name="accept">"Hotovo"</string>
    <!-- button to discard the current recording and return to the caller -->
    <string name="discard">"Zahodiť"</string>
    <!-- acknowlegement button in a number of dialogs -->
    <string name="button_ok">"OK"</string>
    <!-- label shown before the user has recorded anything -->
    <string name="press_record">"Stlačte: 0-MIC 1-Rx 2-Tx+Rx 3-AMR 4-EVRC 5-QCELP  6-AAC_MP4 9-AMR-WB A-AMR-WB-3GPP a stlačte nahrávanie"</string>
    <string name="press_record2">"Stlačte nahrávanie"</string>
    <!-- label shown before the user has recorded anything -SSR support added-->
    <string name="press_record_ssr">"Stlačte: 0-MIC 1-Rx 2-Tx+Rx 3-AMR 4-EVRC 5-QCELP  6-AAC_MP4 7-AAC_SSR 8-LPCM_SSR 9-AMR-WB A-AMR-WB-3GPP a stlačte nahrávanie"</string>
    <!-- the name under which recordings will be visible in the media database is formatted like this -->
    <string name="audio_db_title_format"><xliff:g id="format">yyyy-MM-dd HH:mm:ss</xliff:g></string>
    <!-- all recordings will show up in the media database with this 'artist' name -->
    <string name="audio_db_artist_name">"Vaše záznamy"</string>
    <!-- all recordings will show up in the media database with this 'album' name -->
    <string name="audio_db_album_name">"Zvukové záznamy"</string>
    <!-- all recordings will show up in the media database in a playlist with this name -->
    <string name="audio_db_playlist_name">"Moje záznamy"</string>
    <!-- shown as the message in a dialog when an error occured because of an error accessing the sd card [CHAR LIMIT=NONE] -->
    <string name="error_sdcard_access" product="nosdcard">"Nedá sa získať prístup k úložisku USB."</string>
    <!-- shown as the message in a dialog when an error occured because of an error accessing the sd card -->
    <string name="error_sdcard_access" product="default">"Nedá sa získať prístup ku karte SD."</string>
    <!-- shown as the message in a dialog when the recording is interrupted internally -->
    <string name="error_record_interrupted">"Nahrávanie prerušené"</string>
    <!-- shown as the message in a dialog when the app encountered an unspecified internal error -->
    <string name="error_app_internal">"Vnútorná chyba aplikácie."</string>
    <!-- shown as the message in a dialog when the app encountered and unsupported format -->
    <string name="error_app_unsupported">"Nepodporovaný formát"</string>
    <!-- shown as the message in a dialog when the recording could not be added to the media database -->
    <string name="error_mediadb_new_record">"Zaznamenaný zvuk sa nepodarilo uložiť."</string>
    <!-- shown as the message in a dialog when the recording cannot be started while in call -->
    <string name="error_mediadb_incall">"Táto možnosť je podporovaná iba počas hovoru"</string>
    <!-- shown as the message in a dialog when voice call recording is started for AAC format -->
    <string name="error_mediadb_aacincall">"Nahrávanie hlasového hovoru AAC nie je podporované"</string>
    <!-- shown as the dialog title when user selects recording format -->
    <string name="format_setting">"Typ súboru"</string>
    <string name="format_setting_aac_item">AAC</string>
    <string name="format_setting_amr_item">"AMR"</string>
    <string name="format_setting_3gpp_item">"3GPP"</string>
    <string name="format_setting_wav_item">"WAV"</string>
    <string name="storage_setting">"Umiestnenie úložiska"</string>
    <string name="storage_setting_sdcard_item">"Karta SD"</string>
    <string name="storage_setting_local_item">"Pamäť telefónu"</string>
    <string name="no_phonestorage">"Pamäť telefónu vybratá"</string>
    <string name="keyboard">"Klávesnica"</string>
    <string name="file_deleted">"Súbor bol odstránený!"</string>
    <string name="in_call_record_error">"Chyba v zázname volania."</string>
    <!-- Label shown during recording -->
    <string name="recording_now">"Nahráva sa..."</string>
    <!-- shown as the message in a dialog when the recording file is saved successfully -->
    <string name="file_saved">"Súbor sa uložil"</string>
    <!-- shown as the message in a dialog when the recording file is discarded -->
    <string name="file_discard">"Súbor sa zahodil"</string></resources>